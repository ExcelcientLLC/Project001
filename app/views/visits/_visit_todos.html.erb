<h2>To Do Progress</h2>
<% @visit.to_dos.each do |to_do| %>
  <% if not to_do.complete %>
    <div>
      <div class="todo_checkbox">
        &nbsp;
      </div>
      <div class="visit_todos">
        <b><%= to_do.title %></b>
        </br>
        <%= to_do.description %>
      </div>
    </div>
  <% end %>
<% end %>
