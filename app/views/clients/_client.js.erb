$('#client').html("<p>");
$('#client').append("</p>");
$('#client').append("<h1>Editing Client</h1>");
$('#client').append("<%= escape_javascript(render 'form') %>");
$('#client').append("<div class=button_format><%= escape_javascript(button_to "Timeline", client_visits_path(@client), :method => :get) %></div>");
$('#client').append("<div class=button_format><%= escape_javascript(button_to "Goals", client_visit_path(@client, @client.getSortedVisits.last), :method => :get) %></div>");
$('#client').append("<div class=delete_format><%= escape_javascript(deleteImageButton client_path(@client), "Are you sure you want to delete this client?") %></div>");
