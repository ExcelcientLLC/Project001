<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @client.name %>
</p>

<p>
  <b>Email:</b>
  <%= @client.email %>
</p>

<p>
  <b>Phone:</b>
  <%= @client.phone %>
</p>

<p>
  <b>Address:</b>
  <%= @client.address %>
</p>

<p>
  <b>City:</b>
  <%= @client.city %>
</p>

<p>
  <b>State:</b>
  <%= @client.state %>
</p>

<p>
  <b>Zip:</b>
  <%= @client.zip %>
</p>

<p>
  <b>Comment:</b>
  <%= @client.comment %>
</p>

<p>
    <% @client.visits.each do |visit| %>
      <tr>
        <td><%= visit.visit_date %></td>
        <td><%= link_to 'Show', client_visit_path(@client, visit) %></td>
      </tr>
    <% end %>
</p>

