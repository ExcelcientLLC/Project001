<div class="header" style=" position: relative;">
  <%= link_to image_tag("prosperionLogo.png"), "/clients" %>
    <div style="float:right;">
      <div style="color: #F4BB3A; float: right;width:100%;height:54px">
      <% if @client && @client.first_name && @client.last_name %>
        <%= getClientHeader @client %>
      <% end %>
      <% if @goal && @goal.client && @goal.client.first_name && @goal.client.last_name %>
        <%= getClientHeader @goal.client %>
      <% end %>
      </div>
      <div class="title_text">
        <p><%= @title.upcase %></p>
      </div>
    </div>
</div>
